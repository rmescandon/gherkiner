{
  "name": "gherkiner-cli",
  "displayName": "Gherkiner CLI",
  "description": "Awesome formatter for Gherkin features as command line interface",
  "version": "0.0.2",
  "publisher": "rmescandon",
  "repository": {
    "type": "git",
    "url": "https://github.com/rmescandon/gherkiner.git"
  },
  "bugs": "https://github.com/rmescandon/gherkiner/issues/",
